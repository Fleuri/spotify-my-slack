{
  "name": "spotify-my-slack",
  "version": "0.0.1",
  "description": "Show what's playing on Spotify as your Slack status",
  "engines": {
    "node": "10.7.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "heroku local",
    "test": "eslint src/",
    "watch": "nodemon --exec \"heroku local\" --signal SIGTERM",
    "heroku-postbuild": "sequelize db:migrate"
  },
  "dependencies": {
    "@slack/client": "^4.3.1",
    "async": "^2.6.1",
    "connect-redis": "^3.3.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.6",
    "material-design-lite": "^1.3.0",
    "newrelic": "^4.6.0",
    "pg": "^7.4.3",
    "request": "^2.87.0",
    "sequelize": "^4.38.0",
    "spotify-web-api-node": "^3.1.1"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "nodemon": "^1.18.3",
    "sequelize-cli": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micthiesen/spotify-my-slack"
  },
  "keywords": [
    "express",
    "heroku",
    "node",
    "slack",
    "spotify"
  ],
  "license": "GPL-3.0-only"
}
